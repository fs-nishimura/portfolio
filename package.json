{
  "name": "nnishimura.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "nnishimura <naoko.nishimura1018@gmail.com>",
  "license": "",
  "scripts": {
    "deps": "rm -rf node_modules && npm install",
    "precommit": "eslint main/src",
    "tags:update": "git fetch --prune git@github.com:nnishimura/nnishimura.io.git '+refs/tags/*:refs/tags/*'",
    "version": "npm run tags:update && conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "watch": "webpack-dev-server --hot",
    "build:dev": "NODE_ENV=development webpack",
    "build": "NODE_ENV=production webpack",
    "build:size": "NODE_ENV=production webpack --json | webpack-bundle-size-analyzer"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "build",
        "ci",
        "chore",
        "revert"
      ],
      "scope": {
        "required": false,
        "allowed": [
          "*"
        ],
        "validate": false,
        "multiple": false
      },
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "",
      "autoFix": false
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "axios": "0.17.1",
    "babel": "6.23.0",
    "babel-core": "^7.0.0-beta.32",
    "babel-polyfill": "6.26.0",
    "babel-loader": "^8.0.0-beta.1",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0",
    "request": "",
    "request-promise": ""
  },
  "devDependencies": {
    "ajv": "6.2.1",
    "babel-eslint": "8.1.2",
    "browser-sync": "2.23.6",
    "compression-webpack-plugin": "1.1.11",
    "conventional-changelog-cli": "1.3.2",
    "conventional-changelog-lint": "2.1.1",
    "es6-loader": "0.2.0",
    "eslint": "3.14.1",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.4.0",
    "gulp": "3.9.1",
    "gulp-autoprefixer": "5.0.0",
    "gulp-babel": "7.0.1",
    "gulp-changed": "",
    "gulp-clean": "0.4.0",
    "gulp-combine-media-queries": "",
    "gulp-csscomb": "",
    "gulp-load-plugins": "",
    "gulp-minify-css": "",
    "gulp-minify-html": "",
    "gulp-plumber": "1.2.0",
    "gulp-rename": "",
    "gulp-rev": "",
    "gulp-sass": "",
    "gulp-uglify": "",
    "gulp-watch": "5.0.0",
    "gulp-webpack": "",
    "html-webpack-plugin": "3.0.6",
    "husky": "0.14.3",
    "jsx-loader": "",
    "prettier": "1.7.3",
    "run-sequence": "2.2.1",
    "uglifyjs-webpack-plugin": "1.2.2",
    "underscore": "",
    "validate-commit-msg": "2.14.0",
    "webpack": "4.1.1",
    "webpack-dev-server": "^3.1.1",
    "workbox-webpack-plugin": "3.0.0"
  }
}
